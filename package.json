{
  "name": "tree-sitter-dart",
  "version": "0.1.0",
  "description": "Tree-Sitter Grammar for Dart.",
  "main": "grammar.js",
  "scripts": {
    "generate": "tree-sitter generate --abi=latest",
    "generate-verbose": "tree-sitter generate --abi=latest --log",
    "test": "tree-sitter generate --abi=latest && tree-sitter test",
    "test-update": "tree-sitter generate && tree-sitter test --update"
  },
  "author": "The Dart project authors",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "tree-sitter-cli": "^0.20.6"
  },
  "dependencies": {
    "nan": "^2.15.0"
  },
  "tree-sitter": [
    {
      "scope": "source.dart",
      "file-types": [
        "dart"
      ],
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ]
}
